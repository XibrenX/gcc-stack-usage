{
	"name": "gcc-stack-usage",
	"displayName": "Gcc stack usage",
	"description": "Show advanced gcc compile information in VSCode",
	"version": "0.1.1",
	"publisher": "Wibren Wiersma",
	"repository": "",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"extensionKind": [
		"workspace"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gcc-advanced.updateSuStore",
				"title": "Reload su files",
				"icon": "$(refresh)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gcc-advanced",
					"title": "Gcc advanced",
					"icon": "$(symbol-structure)"
				}
			]
		},
		"views": {
			"gcc-advanced": [
				{
					"id": "gcc-advanced.globalStackUsage",
					"name": "Global stack usage"
				},
				{
					"id": "gcc-advanced.currentFileStackUsage",
					"name": "Current file stack usage"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "gcc-advanced.globalStackUsage",
				"contents": "No su files currently loaded. Compile with `-fstack-usage` flag and hit [reload](command:gcc-advanced.updateSuStore)."
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "gcc-advanced.updateSuStore",
					"when": "view == gcc-advanced.globalStackUsage",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.3.2"
	}
}
